{
    "name": "stonekeep/claimit",
    "description": "ClaimIt web application",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "aws/aws-sdk-php": "^3.356",
        "google/apiclient": "^2.15",
        "robmorgan/phinx": "^0.16.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "ClaimIt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClaimIt\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "sort-packages": true,
        "platform": {
            "php": "8.3.6"
        }
    },
    "minimum-stability": "stable"
}
